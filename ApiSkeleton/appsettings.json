{
  "JwtOptions": {
    "Issuer": "your-app",
    "Audience": "your-app-client",
    "Key": "SUPER_SECRET_KEY_MIN_32_CHARS",
    "ExpiresMinutes": 60,
    "AccessTokenMinutes": 15,
    "RefreshTokenDays": 7
  },
  "CorsOptions": {
    "PolicyName": "AllowMyFrontEnd",
    "AllowedOrigins": [
      "http://localhost:5173",
      "https://frontendku.com"
    ]
  },
  "DatabaseOptions": {
    "ConnectionString": "Host=localhost;Port=5432;Database=ApiSkeleton;Username=postgres;Password=root;SslMode=Prefer;TrustServerCertificate=True;"
  },
  "DatabaseInitialization": {
    "EnableMigration": true,
    "EnableSeed": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
